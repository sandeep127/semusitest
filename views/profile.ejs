<head>
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
	<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script> 
</head>
<body>
 	<nav class="navbar navbar-inverse">
	  	<div class="container-fluid">
	    	<div class="navbar-header">
	      		<a class="navbar-brand" >Semusi Technologies</a>
	    	</div>
	    	<span class="nav navbar-nav navbar-right"><h3 style="color:white;"><%=data%>
	    	<a href="/logout"><button type="button" class="btn btn-danger">LOGOUT</button></a></h3>
	    	</span>
	  	</div>
	</nav>
	<center><h2>Details of the Data Present</h2></center>
	<div class="col-lg-2"></div>
	<div class="col-lg-8">
		<table class="table" style="border: 1px solid grey; margin-top:50px;">
			<tr><th>S. No.</th><th>Name</th><th>City</th><th>Phone Number</th><th>E mail</th></tr>
			<%for(var i=0;i<doc.length;i++){%>
			<tr>
				<td><%=i+1%></td>
				<td><%=doc[i].name%></td>
				<td><%=doc[i].state%></td>
				<td><%=doc[i].phone%></td>
				<td><%=doc[i].email%></td>
			</tr>
			<% } %>
		</table>
		<a href="/adduser"><button type="button">Add User</button></a>
	</div>
</body>